{
  "name": "fix for recreation.gov",
  "version": "0.1",
  "manifest_version": 2,
  "description": "remove annoying footer in middle of page",
  "homepage_url": "https://github.com/helgasoft/chrome-recgov",
  "icons": { "16": "rec.gov.png" },
  "browser_action": {},
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "https://*.recreation.gov/*",
    "http://*.recreation.gov/*"
 ]
}